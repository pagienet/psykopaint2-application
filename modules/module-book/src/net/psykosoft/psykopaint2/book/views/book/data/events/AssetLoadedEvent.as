package net.psykosoft.psykopaint2.book.views.book.data.events{	import flash.events.Event;		public class AssetLoadedEvent extends Event {		 		public static const ASSET_LOADED:String = "assetloaded";		public static const ASSET_LOAD_PROGRESS:String = "assetloadprogress";		public static const ASSET_LOAD_ERROR:String = "assetloadederror";		public static const ASSET_BINARY_LOADED:String = "assetbinaryloaded";				private var _data:*;		private var _customData:Object;		private var _percent:Number;		private var _message:String;				function AssetLoadedEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false){			super(type, bubbles, cancelable);		}				public function set data(val:*):void		{			_data = val;		}		public function get data():*		{			return _data;		}		public function set customData(val:Object):void		{			_customData = val;		}		public function get customData():Object		{			return _customData;		}				public function set percent(val:Number):void		{			_percent = val;		}				public function get percent():Number		{			return _percent;		}				public function set message(val:String):void		{			_message = val;		}				public function get message():String		{			return _message;		}			}} 